<h1>Posts:</h1>

<% @posts.each do |post| %>
  <%= calc_points(post.id)%>
  <%= render :partial => "votes/form", :locals => {:post_id => post.id}%>
  <%= link_to post.title, set_url(post.url) %>
  <%= link_to "discuss", post_url(post) %>
  <br>
  <br>
<% end %>

<%= link_to "Create New Post", new_post_url%>